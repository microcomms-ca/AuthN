@page
@model Client.Pages.SecureModel
@{
}

@if(User.Identity.IsAuthenticated)
{
    <p>User authenticated</p>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Claim</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            
    @foreach(var claim in User.Claims)
    {
        <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        
    }
        </tbody>
    </table>
}
else
{
    <p>Not authenticated</p>
}